{
    "manifest_version" : 2,
    "name": "VPN Blocker",
    "description": "This extension aims to detect and block VPN usage",
    "version" : "1.0",
    "browser_action":{
        "default_popup":"popup.html"
    },
    "background":{"scripts":["background.js"]},
    "permissions":[
        "webRequest",
        "<all_urls>",
        "webRequestBlocking"
            ]
}
